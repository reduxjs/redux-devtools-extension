{
  "name": "@reduxjs/redux-devtools-extension-fork",
  "version": "0.0.1",
  "description": "Wrappers for Redux DevTools Extension.",
  "main": "dist/index.js",
  "module": "dist/redux-devtools-extension-fork.esm.js",
  "unpkg": "dist/redux-devtools-extension-fork.umd.min.js",
  "types": "dist/typings.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/reduxjs/redux-devtools-extension"
  },
  "homepage": "https://github.com/reduxjs/redux-devtools-extension",
  "author": "Mihail Diordiev <zalmoxisus@gmail.com> (https://github.com/zalmoxisus)",
  "license": "MIT",
  "scripts": {
    "build-ci": "tsdx build --format cjs,esm,umd --name redux-devtools-extension-fork && api-extractor run",
    "build": "tsdx build --format cjs,esm --name redux-devtools-extension-fork && api-extractor run --local",
    "dev": "tsdx watch  --format cjs,esm",
    "format": "prettier --write \"src/*.ts\" \"**/*.md\"",
    "format:check": "prettier --list-different \"src/*.ts\" ",
    "lint": "tsdx lint src",
    "prepare": "npm run lint && npm run format:check && npm test && npm run build-ci",
    "test": "tsdx test"
  },
  "files": [
    "dist/*.js",
    "dist/*.js.map",
    "dist/typings.d.ts",
    "src"
  ],
  "peerDependencies": {
    "redux": "^3.1.0 || ^4.0.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.7.8",
    "@types/jest": "^25.1.3",
    "prettier": "^1.19.1",
    "redux": "^4.0.5",
    "tsdx": "^0.12.3"
  }
}
